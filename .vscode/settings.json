{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.options": {
        "extensions": [
            ".js",
            ".ts",
            ".tsx",
            ".html"
        ]
    },
    "eslint.validate": [
        "html",
        "javascript",
        "react",
        "typescript",
        "typescriptreact"
    ]
}